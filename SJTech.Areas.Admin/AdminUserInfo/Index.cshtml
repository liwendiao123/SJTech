@page "{handler?}"
@model Senparc.Areas.Admin.Areas.Admin.Pages.AdminUserInfo_IndexModel
@{
}
@{
    ViewData["Title"] = "管理员管理";
}
@section breadcrumbs{

}
@section scripts{
    <script src="/js/Admin/Pages/AdminUserInfo/Index.js"></script>
    <script>
    
        $(function () {
            vm.list =@Html.Raw(Model.AdminUserInfoList.Select(z => new
             {
                 z.Id,
                 z.UserName,
                 z.Note,
                 AddTime = z.AddTime.ToString("yyyy-MM-dd")
             }).ToJson());
            });
    </script>
}
<el-table :data="list"
          ref="multipleTable"
          tooltip-effect="dark"
          style="width: 100%"
          @@selection-change="handleSelectionChange">
    <el-table-column type="selection"
                     width="55">
    </el-table-column>
    <el-table-column prop="AddTime"
                     label="日期"
                     width="180">
    </el-table-column>
    <el-table-column prop="UserName"
                     label="用户名"
                     width="180">
    </el-table-column>
    <el-table-column prop="Note"
                     label="备注"
                     width="180">
    </el-table-column>
    <el-table-column prop="AddTime"
                     label="添加时间">
    </el-table-column>
    <el-table-column prop="address"
                     label="操作">
        <template slot-scope="scope">
            <el-button size="mini"
                       @@click="handleEdit(scope.row)"><i class="el-icon-edit"></i></el-button>
        </template>
    </el-table-column>
</el-table>
<div style="text-align:center;margin-top:10px;">
    <el-pagination @@size-hange="handleSizeChange"
                   @@current-change="handleCurrentChange"
                   :current-page="currentPage4"
                   :page-sizes="[100, 200, 300, 400]"
                   :page-size="100"
                   layout="total, sizes, prev, pager, next, jumper"
                   :total="400">
    </el-pagination>
</div>

